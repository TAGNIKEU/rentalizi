<odoo>
    <data>
        <record id="rentalizi_ville_tree_view" model="ir.ui.view">
            <field name="name">rentalizi.ville.tree.view</field>
            <field name="model">rentalizi.ville</field>
            <field name="arch" type="xml">
                <tree string="Liste des villes">
                    <field name="nom"/>
                </tree>
            </field>
        </record>
        <record id="action_villes_rentalizi" model="ir.actions.act_window">
            <field name="name">Villes</field>
            <field name="res_model">rentalizi.ville</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>